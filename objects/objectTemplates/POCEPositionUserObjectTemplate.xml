                   return "DEPARTMENT|" + organization  + "|" + organizationalUnit + "|" + name + "|" + person_employment_parent + "|" + personalNumber + "|manager"}
                   else
                   {return "DEPARTMENT|" + organization  + "|" + organizationalUnit + "|" + name + "|" + person_employment_parent + "|" + personalNumber + "|default"}</code>
                </script>
            </expression>
            <target>
