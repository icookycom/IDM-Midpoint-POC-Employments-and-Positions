        <inducement id="1226">
            <focusMappings>
                <mapping id="7">
                    <name>001</name>
                    <documentation>get from user Forward Roles the department manager</documentation>
                    <authoritative>true</authoritative>
                    <strength>strong</strength>
                    <expression>
                        <script>
                            <code>
 user_froles = midpoint.findLinkedSources('from Project User to Org Dep').collect { basic.getExtensionPropertyValues(it, "http://example.com/xml/ns/mySchema", "person_forward_roles")}
result = []
for (i in user_froles)
{
for (ii in i)
{
if (ii.startsWith("DEPARTMENT|"))
{
iii = ii.tokenize( '|' )
if (iii[6] == "manager")
{
result.add(iii[5])}

log.info "AAAAAAAAAAAAAA " + result
}
}

}

return result
</code>
                        </script>
                    </expression>
                    <target>
                        <path>$focus/extension/org_boss</path>
                        <set>
                            <predefined>all</predefined>
                        </set>
                    </target>
                </mapping>
            </focusMappings>
        </inducement>
